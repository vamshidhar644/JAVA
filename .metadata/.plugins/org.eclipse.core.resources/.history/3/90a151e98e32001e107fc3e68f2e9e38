package inner_class;

class Outer{
	int a; // member variable
	
	public void show() {  // member method
		
	}
	
	class Inner{ // member class
		public void display() {
			System.out.println("In Display");
		}
	}
}
public class InnerClass {
	
	public static void main(String[] args) {
		Outer obj = new Outer();
		obj.show();
		
		Outer.Inner obj1 = obj.new Inner();
	}

}
