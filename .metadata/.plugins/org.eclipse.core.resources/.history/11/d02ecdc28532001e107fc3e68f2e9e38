package static_keyword;

class Emp{
	int eid;
	int salary;
	static String ceo;
	
	public void show() {
		System.out.println(eid + " : " + salary + " : " + ceo);
	}
}
public class StaticKeyword {
	public static void main(String[] args) {
		Emp vamshi = new Emp();
		vamshi.eid = 101;
		vamshi.salary = 40000;
		vamshi.ceo = "Mahesh";
		
		Emp rahul = new Emp();
		rahul.eid = 109;
		rahul.salary = 50000;
		
		rahul.ceo = "Navin";
		
		vamshi.show();
		rahul.show();
	}
}
