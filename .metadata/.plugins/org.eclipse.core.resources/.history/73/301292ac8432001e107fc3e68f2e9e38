package static_keyword;

class Emp{
	int eid;
	int salary;
	String ceo;
}
public class StaticKeyword {
	public static void main(String[] args) {
		Emp vamshi = new Emp();
		vamshi.eid = 01;
		vamshi.salary = 40000;
		vamshi.ceo = "Rahul";
		
		Emp rahul = new Emp();
		rahul.eid = 09;
		rahul.salary = 50000;
		
	}
}
