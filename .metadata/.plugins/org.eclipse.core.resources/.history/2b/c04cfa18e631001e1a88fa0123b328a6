package classes_and_objects;

class Calcc{
	int __num1;
	int __num2;
	int __result;
	
	public Calcc() { // with parameter
		__num1 = 5;
		__num2 = 5;
		System.out.println("In Constructor");
	}
	public Calcc(int n) { // without parameter
		System.out.println(n);
	}
}

public class Constructor {
	public static void main(String[] args) {
		Calcc obj = new Calcc(7); // constructor is also called
		
		System.out.println(obj.__num1);
	}
}
