package classes_and_objects;

class Calc{
	int __num1;
	int __num2;
	int __result;
	
	void perform(){ // this is a method
		__result = __num1 + __num2;
		System.out.println(__result);
	}
}

public class Object {
	public static void main(String[] args) {
		Calc obj = new Calc(); // Calc() is an object here
		
		obj.perform();
	}
}
