package anonymous;

class A{
	public void show() {
		System.out.println("In A show()");
	}
}

class B extends A{
	public void show() {
		System.out.println("I am the best");
	}
}
public class Anonymous {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		A obj = new B();
		obj.show();
	}

}
