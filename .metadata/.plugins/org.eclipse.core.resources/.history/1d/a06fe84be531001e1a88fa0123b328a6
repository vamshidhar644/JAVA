package classes_and_objects;

class Calcc{
	int __num1;
	int __num2;
	int __result;
	
	
}

public class Constructor {
	public static void main(String[] args) {
		Calcc obj = new Calcc(); // constructor
		
		obj.__num1 = 4;
		obj.__num2 = 5;
		
		int k = obj.perform(2, 6);
		
		System.out.println(k);
	}
}
