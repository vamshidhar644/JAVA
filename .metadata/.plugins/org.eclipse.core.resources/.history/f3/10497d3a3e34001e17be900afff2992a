package overriding;

class Aa{
	int i;
	public void show() {
		System.out.println("In A "+ i);
	}
}
class Bb extends Aa{

	public void show() {
		i=9;
		super.i = 4;
		super.show(); // calls show() in super class
		System.out.println("In B "+ i);
	}
}
public class MethodDispatch {
	public static void main(String[] args) {
		Bb obj = new Bb();
		
		obj.show();
	}
}
