package anonymous;


class Aa{
	public void show() {
		System.out.println("In A show()");
	}
}
public class AnonymousInterface {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		A obj = new A() {
			public void show() {
				System.out.println("I am the best");
			}
		};
		obj.show();
	}

}
