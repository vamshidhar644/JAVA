package Encapsulation;

class Student{
	int __rollno;
	String __name;
	
	public void setRoll(int r) {
		__rollno = r;
	}
	public void getRoll() {
		return __rollno;
	}
}
public class Encapsulation {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Student st = new Student();
		
		st.__name = "Vamshi";
		st.__rollno = 3;
	}

}
