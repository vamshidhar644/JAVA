package loops;

public class NestedLoop {
	public static void main(String[] args) {
//		for(int i=1;i<=6;i++) {
//			for(int j=1; j<i+1; j++) {
//				System.out.print(j+" ");
//			}
//			System.out.println();
//		}
		
		 for (int i = 1; i <= 3; i++) {
	            char ch = 'A';
	            for (int j = 1; j <= i; j++) {
	                System.out.print(ch + " ");
	                ch++;
	            }
	            System.out.println();
	        }
	}
}
