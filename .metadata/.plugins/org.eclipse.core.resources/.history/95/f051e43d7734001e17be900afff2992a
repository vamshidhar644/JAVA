package final_keyword;

 class Aa{
	public void show(int i) {
		System.out.println("In A Show()");
	}
}
class Bb extends Aa{
	public void show() {
		System.out.println("In B show()");
	}
}
public class FinalClass {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Bb obj = new Bb();
		obj.show(4);
	}

}
