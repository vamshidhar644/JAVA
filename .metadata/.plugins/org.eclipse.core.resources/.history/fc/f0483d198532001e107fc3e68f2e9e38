package static_keyword;

class Emp{
	int eid;
	int salary;
	String ceo;
	
	public void show() {
		System.out.println("eid: " + eid + " salary: " + salary + " ceo: " + ceo);
	}
}
public class StaticKeyword {
	public static void main(String[] args) {
		Emp vamshi = new Emp();
		vamshi.eid = 1;
		vamshi.salary = 40000;
		vamshi.ceo = "Mahesh";
		
		Emp rahul = new Emp();
		rahul.eid = 9;
		rahul.salary = 50000;
		rahul.ceo = "Mahesh";
		
		vamshi.show();
		rahul.show();
	}
}
