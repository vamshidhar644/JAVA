package Variables;

public class variables_2 {
	public static void main(String args[]) {
		// int , float, double
		
		int int__a = 5; // 4 bytes => 32bits => -2,147,483,648 to 2,147,483,648
		short short__a = 4; // 2bytes => 12bits ==> -32768 to 32768
		
		
		System.out.println("int: "+int__a);
		Syste.out.println("short: "+short__a)
	}
}
